/* XPECT_SETUP eu.numberfour.n4js.spec.tests.N4JSSpecTest END_SETUP */

class A {
	public set s(s : string) { }
}

var an : A;
var as : ~A;
var afs : ~~A;
var aro : ~r~A;
var awo : ~w~A;
var ai : ~i~A;

// XPECT noerrors --> ""
aro = an;
// XPECT noerrors --> ""
aro = as;
// XPECT noerrors --> ""
aro = afs;
// XPECT noerrors --> ""
aro = aro;
// XPECT noerrors --> ""
aro = awo;
// XPECT noerrors --> ""
aro = ai;

// XPECT noerrors --> ""
awo = an;
// XPECT noerrors --> ""
awo = as;
// XPECT noerrors --> ""
awo = afs;
// XPECT errors --> "~r~A is not a structural subtype of ~w~A: missing setter s; s failed: writable field requires a setter in subtype." at "aro"
awo = aro;
// XPECT noerrors --> ""
awo = awo;
// XPECT errors --> "~i~A is not a structural subtype of ~w~A: missing setter s; s failed: writable field requires a setter in subtype." at "ai"
awo = ai;

// XPECT errors --> "A is not a structural subtype of ~i~A: missing setter s; s failed: non-optional writable field requires a readable field or a getter in subtype." at "an"
ai = an;
// XPECT errors --> "~A is not a structural subtype of ~i~A: missing setter s; s failed: non-optional writable field requires a readable field or a getter in subtype." at "as"
ai = as;
// XPECT errors --> "~~A is not a structural subtype of ~i~A: missing setter s; s failed: non-optional writable field requires a readable field or a getter in subtype." at "afs"
ai = afs;
// XPECT errors --> "~r~A is not a structural subtype of ~i~A: missing setter s; s failed: non-optional writable field requires a readable field or a getter in subtype." at "aro"
ai = aro;
// XPECT errors --> "~w~A is not a structural subtype of ~i~A: missing setter s; s failed: non-optional writable field requires a getter in subtype." at "awo"
ai = awo;
// XPECT errors --> "~i~A is not a structural subtype of ~i~A: missing setter s; s failed: non-optional writable field requires a readable field or a getter in subtype." at "ai"
ai = ai;
