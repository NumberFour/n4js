/* XPECT_SETUP eu.numberfour.n4js.spec.tests.N4JSSpecTest END_SETUP */


interface A {}
interface B {}

let a:A;
let b:B;

let aub:A|B;

interface I1 {
	m1:A;
	get m2(): A;
}

interface I2 {
	get m1(): A;
	m2: A;
}

interface I3 {
	get m1(): B;
	m2: B;
}


var i13: I1&I3;
// set m1(a:A)
// get m1():A&B
// set m2(b:B)
// get m2():A&B

// XPECT noerrors
i13.m1;

// XPECT type of 'i13.m1' --> intersection{A,B}
i13.m1;

// XPECT noerrors
i13.m1 = a;

// XPECT errors --> "B is not a subtype of A." at "b"
