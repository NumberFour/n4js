class C {
	public s : string;
	n : number;
	
	constructor(spec : ~~this with {n?: number;}) {
		this.s = spec.s;
		if (spec.n) {
			this.n = spec.n;
		} else {
			this.n = 1;
		}
		
	}
}

var ca = new C({s:"Hello", n:42});
console.log(ca.s)
console.log(ca.n)

var cb = new C({s:"Hello"});
console.log(cb.s)
console.log(cb.n)

var cc = new C({s:"Hello", superfluous: "nobody needs that", n: 42});
console.log(cc.s)
console.log(cc.n)
