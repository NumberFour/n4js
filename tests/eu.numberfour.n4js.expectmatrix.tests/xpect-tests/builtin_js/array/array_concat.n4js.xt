// Array.prototype.concat()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array/concat

// Concatenating two arrays
var numeric1 = [3, 4, 5];
var numeric2 = [1, 2, 3];

var numeric12 = numeric1.concat(numeric2);
console.log(numeric12);

// Concatenating three arrays
var num1 = [1, 2, 3];
var num2 = [4, 5, 6];
var num3 = [7, 8, 9];

var nums = num1.concat(num2, num3);
console.log(nums);

// Concatenating values to an array
var num : Array<number> = [1, 2, 3];
var numeric = num.concat(1, 2, 3);
console.log(numeric);

