/* XPECT_SETUP eu.numberfour.n4js.tests.N4JSXpectTest END_SETUP */


// ------------------------------------
// hiding types:

class C {}
interface R {}
interface I {}
// XPECT warnings --> "The type parameter C hides class C." at "C"
class A1<C> {}
// XPECT warnings --> "The type parameter R hides interface R." at "R"
class A2<R> {}
// XPECT warnings --> "The type parameter I hides interface I." at "I"
class A3<I> {}


// ------------------------------------
// hiding a type variable:

class B<T> {
	
	// XPECT warnings --> "The type parameter T hides type variable T." at "T"
	<T> m1() : void {}
	
	m2() : void {
		// XPECT warnings --> "The type parameter T hides type variable T." at "T"
		var f = function <T> () {};
		
		// XPECT errors --> "Unsupported feature: class expressions." at "class"
		var c = class {
			// XPECT warnings --> "The type parameter T hides type variable T." at "T"
			f = function <T> () {};
			// XPECT warnings --> "The type parameter T hides type variable T." at "T"
			<T> mInner() : void {}
		};
		
	}
	
	// ok:
	
	static <T> m3() : void {}
	
	static m4() : void {
		var f = function <T> () {};
		
		// XPECT errors --> "Unsupported feature: class expressions." at "class"
		var c = class {
			f = function <T> () {};
			<T> mInner() : void {}
		};
	}
}


// ------------------------------------
// no warning if hiding something other than a type or type variable:

var myVar : string;
class Clazz<myVar> {}

