// ------------------------------------------------
// passing on a type variable as a type argument

class G2a<S1, S2,S3> {}
class G2b<S1,+S2,S3> {}
class G2c<S1,-S2,S3> {}

// XPECT noerrors -->
class C21a<T> extends G2a<string,T,number> {}
// XPECT noerrors -->
class C21b<T> extends G2b<string,T,number> {}
// XPECT noerrors -->
class C21c<T> extends G2c<string,T,number> {}

class C22a<+T>
	// XPECT errors --> "Cannot use covariant type variable at invariant position." at "T"
	extends G2a<string,T,number> {}
class C22b<+T>
	// XPECT noerrors -->
	extends G2b<string,T,number> {}
class C22c<+T>
	// XPECT errors --> "Cannot use covariant type variable at contravariant position." at "T"
	extends G2c<string,T,number> {}

class C23a<-T>
	// XPECT errors --> "Cannot use contravariant type variable at invariant position." at "T"
	extends G2a<string,T,number> {}
class C23b<-T>
	// XPECT errors --> "Cannot use contravariant type variable at covariant position." at "T"
	extends G2b<string,T,number> {}
class C23c<-T>
	// XPECT noerrors -->
	extends G2c<string,T,number> {}

