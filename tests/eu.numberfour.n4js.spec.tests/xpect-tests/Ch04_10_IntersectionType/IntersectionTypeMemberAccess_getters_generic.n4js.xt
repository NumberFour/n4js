/* XPECT_SETUP eu.numberfour.n4js.spec.tests.N4JSSpecTest END_SETUP */

interface A {}
interface B {}

let a:A;
let b:B;

let ab:A|B;

interface I1<T> {
	get onlyIn1(): T;
	get inBothDiffType(): T;
}

interface I2<T> {
	get onlyIn2(): T;
	get inBothDiffType(): T;
}

var i12 : I1<A> & I2<B>;

// get onlyIn1(): A

// XPECT noerrors
i12.onlyIn1;

// XPECT type of 'i12.onlyIn1' --> A
i12.onlyIn1;

// XPECT errors --> "The accessor onlyIn1 is read-only." at 'onlyIn1'
i12.onlyIn1 = a;



// get onlyin2(): B;

// XPECT noerrors
i12.onlyIn2;

// XPECT type of 'i12.onlyIn2' --> B
i12.onlyIn2;

// XPECT errors --> "The accessor onlyIn2 is read-only." at 'onlyIn1'
i12.onlyIn2 = b;



// get inBothDiffType(): A&B

// XPECT noerrors
i12.inBothDiffType;

// XPECT type of 'i12.inBothDiffType' --> intersection{A,B}
i12.inBothDiffType;

// XPECT errors --> "The accessor inBothDiffType is read-only." at "inBothDiffType"
i12.inBothDiffType = ab;
